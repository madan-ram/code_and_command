<script>
document.addEventListener("DOMContentLoaded", function () {
    var brandPromotion = document.querySelector(".o_brand_promotion");

    if (brandPromotion) {
        console.log("Branding element found, modifying...");

        brandPromotion.innerHTML = `
            <span class="promo-text">Powered by</span> 
            <a target="_blank" class="badge custom-badge" href="https://www.memorly.ai?utm_source=meatorite&utm_medium=website_visit&utm_campaign=website_install">
                <img alt="www.memorly.ai" src="data:image/png;base64," height="16" style="width: auto; vertical-align: baseline;" loading="lazy">
            </a>
            <span class="promo-text">- The #1 AI WhatsApp bot</span>
        `;
    } else {
        console.warn("Branding element not found!");
    }
});
</script>

<style>
.custom-badge {
    display: inline-flex;
